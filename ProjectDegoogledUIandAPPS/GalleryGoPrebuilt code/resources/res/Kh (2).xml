<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <android.widget.FrameLayout android:id="@+id/inner_view" android:paddingLeft="@dimen/photosgo_videotrimming_trimview_handle_halfsize" android:paddingRight="@dimen/photosgo_videotrimming_trimview_handle_halfsize" android:clipChildren="false" android:clipToPadding="false" android:layout_width="match_parent" android:layout_height="match_parent">
