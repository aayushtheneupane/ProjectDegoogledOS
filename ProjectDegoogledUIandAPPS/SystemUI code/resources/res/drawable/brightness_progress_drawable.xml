<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:gravity="left|right|center_vertical|center_horizontal|fill_horizontal|center" android:id="@android:id/background">
        <shape android:tint="?android:attr/colorControlActivated" android:shape="rectangle">
            <size android:height="@dimen/seek_bar_height"/>
            <solid android:color="@color/white_disabled"/>
            <corners android:radius="@dimen/seek_bar_corner_radius"/>
        </shape>
    </item>
    <item android:gravity="left|right|center_vertical|center_horizontal|fill_horizontal|center" android:id="@android:id/progress">
        <scale android:scaleWidth="100%">
            <shape android:tint="?android:attr/colorControlActivated" android:shape="rectangle">
                <size android:height="@dimen/seek_bar_height"/>
                <solid android:color="@android:color/white"/>
                <corners android:radius="@dimen/seek_bar_corner_radius"/>
            </shape>
        </scale>
    </item>
</layer-list>
