<?xml version="1.0" encoding="utf-8"?>
<com.android.systemui.statusbar.StatusBarMobileView xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:systemui="http://schemas.android.com/apk/res/com.android.systemui" xmlns:aapt="http://schemas.android.com/aapt" android:gravity="center_vertical" android:id="@+id/mobile_combo" android:layout_width="wrap_content" android:layout_height="match_parent">
    <com.android.keyguard.AlphaOptimizedLinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/mobile_group" android:layout_width="wrap_content" android:layout_height="match_parent">
        <ImageView android:layout_gravity="center_vertical" android:id="@+id/mobile_volte" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:paddingStart="4dp" android:paddingEnd="3dp"/>
        <FrameLayout android:layout_gravity="center_vertical" android:id="@+id/inout_container" android:layout_width="wrap_content" android:layout_height="17dp">
            <ImageView android:id="@+id/mobile_in" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/ic_activity_down" android:paddingEnd="2dp"/>
            <ImageView android:id="@+id/mobile_out" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/ic_activity_up" android:paddingEnd="2dp"/>
        </FrameLayout>
        <ImageView android:layout_gravity="center_vertical" android:id="@+id/mobile_type" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:paddingStart="2.5dp" android:paddingEnd="1.5dp"/>
        <Space android:id="@+id/mobile_roaming_space" android:visibility="gone" android:layout_width="@dimen/roaming_icon_start_padding" android:layout_height="match_parent"/>
        <FrameLayout android:layout_gravity="center_vertical" android:id="@+id/mobile_signal_type" android:layout_width="@dimen/status_bar_mobile_signal_width" android:layout_height="@dimen/status_bar_mobile_signal_with_type_width">
            <com.android.systemui.statusbar.AnimatedImageView android:layout_gravity="right|center_vertical|center_horizontal|center|end" android:id="@+id/mobile_signal" android:layout_width="wrap_content" android:layout_height="wrap_content" app:hasOverlappingRendering="false"/>
            <ImageView android:layout_gravity="center_vertical" android:id="@+id/mobile_roaming" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/stat_sys_roaming" android:contentDescription="@string/data_connection_roaming"/>
            <ImageView android:layout_gravity="top|left|center_vertical|center_horizontal|center|start" android:id="@+id/mobile_type_small" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="@dimen/status_bar_mobile_type_height" android:src="@drawable/stat_sys_roaming" android:scaleType="fitStart"/>
        </FrameLayout>
    </com.android.keyguard.AlphaOptimizedLinearLayout>
</com.android.systemui.statusbar.StatusBarMobileView>
