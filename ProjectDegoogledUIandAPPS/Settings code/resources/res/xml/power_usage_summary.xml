<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:settings="http://schemas.android.com/apk/res/com.android.settings" xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:title="@string/power_usage_summary_title" android:key="power_usage_summary_screen" app:keywords="@string/keywords_battery">
    <com.android.settingslib.widget.LayoutPreference android:layout="@layout/battery_header" android:title="@string/summary_placeholder" android:selectable="true" android:key="battery_header" app:controller="com.android.settings.fuelgauge.BatteryHeaderPreferenceController"/>
    <com.android.settings.widget.CardPreference android:title="@string/summary_placeholder" android:key="battery_tip" app:controller="com.android.settings.fuelgauge.batterytip.BatteryTipPreferenceController"/>
    <Preference android:title="@string/battery_saver" android:key="battery_saver_summary" android:fragment="com.android.settings.fuelgauge.batterysaver.BatterySaverSettings" app:controller="com.android.settings.fuelgauge.BatterySaverController" app:platform_slice="true"/>
    <Preference android:title="@string/smart_battery_manager_title_custom" android:key="smart_battery_manager" android:fragment="com.android.settings.fuelgauge.SmartBatterySettings" app:controller="com.android.settings.fuelgauge.batterytip.BatteryManagerPreferenceController"/>
    <com.havoc.support.preferences.SwitchPreference android:title="@string/battery_percentage" android:key="battery_percentage" android:summary="@string/battery_percentage_description" app:controller="com.android.settings.display.BatteryPercentagePreferenceController"/>
    <com.havoc.support.preferences.SystemSettingMasterSwitchPreference android:title="@string/smart_charging_title" android:key="smart_charging" android:summary="@string/smart_charging_summary" android:defaultValue="false" android:fragment="com.android.settings.fuelgauge.smartcharging.SmartChargingSettings"/>
    <com.android.settings.fuelgauge.PowerGaugePreference android:title="@string/battery_last_full_charge" android:selectable="false" android:key="last_full_charge" app:allowDividerAbove="true"/>
    <com.android.settings.fuelgauge.PowerGaugePreference android:title="@string/device_screen_usage" android:selectable="false" android:key="screen_usage"/>
    <com.android.settings.fuelgauge.PowerGaugePreference android:title="@string/battery_temp_title" android:selectable="false" android:key="battery_temp"/>
</PreferenceScreen>
