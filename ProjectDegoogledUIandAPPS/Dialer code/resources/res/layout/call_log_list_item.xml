<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:id="@+id/call_log_list_item" android:layout_width="match_parent" android:layout_height="wrap_content">
    <TextView android:layout_gravity="start" android:id="@+id/call_log_day_group_label" android:paddingTop="@dimen/call_log_day_group_padding_top" android:paddingBottom="@dimen/call_log_day_group_padding_bottom" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginStart="@dimen/call_log_start_margin" android:layout_marginEnd="@dimen/call_log_outer_margin" style="@style/Dialer.TextAppearance.OVERLINE.Ellipsize"/>
    <android.support.p002v7.widget.CardView android:id="@+id/call_log_row" style="@style/CallLogCardStyle">
        <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
            <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/primary_action_view" android:background="?android:attr/selectableItemBackground" android:paddingTop="@dimen/call_log_vertical_padding" android:paddingBottom="@dimen/call_log_vertical_padding" android:focusable="true" android:nextFocusLeft="@+id/quick_contact_photo" android:nextFocusRight="@+id/call_back_action" android:layout_width="match_parent" android:layout_height="wrap_content" android:paddingStart="@dimen/call_log_start_margin" android:paddingEnd="@dimen/call_log_outer_margin">
